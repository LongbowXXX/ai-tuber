{
  "name": "vtube-stage",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:electron": "VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "build": "tsc -b && vite build",
    "build:electron-main": "tsc -p tsconfig.electron.json",
    "build:electron": "npm run build && npm run build:electron-main",
    "start:electron": "npm run build:electron && electron .",
    "build:win": "npm run build:electron && electron-builder --win --config electron-builder.yml",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview"
  },
  "main": "dist-electron/main.js",
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@modelcontextprotocol/sdk": "^1.26.0",
    "@mui/icons-material": "^7.0.2",
    "@mui/material": "^7.0.2",
    "@pixiv/three-vrm": "^3.4.0",
    "@pixiv/three-vrm-animation": "^3.4.0",
    "@react-three/drei": "^10.0.6",
    "@react-three/fiber": "^9.1.2",
    "@react-three/postprocessing": "^3.0.4",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "reflect-metadata": "^0.2.2",
    "remark-gfm": "^4.0.1",
    "styled-components": "^6.1.18",
    "three": "^0.175.0",
    "zod": "^3.25.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/three": "^0.175.0",
    "@vitejs/plugin-react": "^4.3.4",
    "electron": "^33.2.0",
    "electron-builder": "^24.13.3",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.3.1"
  }
}
