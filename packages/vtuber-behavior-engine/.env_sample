# This is a sample .env file. Copy this file to .env and fill in the values.

# Path to the vtube-stage executable (Electron app)
# For development, use the electron command with the package path
# Example (Windows): VTUBE_STAGE_EXECUTABLE="electron"
# Example (Linux/Mac): VTUBE_STAGE_EXECUTABLE="electron"
# Set the working directory to the vtube-stage package directory
VTUBE_STAGE_EXECUTABLE="electron"

# Google
GOOGLE_API_KEY="YOUR_API_KEY_HERE"
GOOGLE_GENAI_USE_VERTEXAI="0"
# Google Speech-to-Text
GOOGLE_APPLICATION_CREDENTIALS="C:\\Users\\your_name\\path\\to\\downloaded-key.json"

# NewsAPI
# If you use google news, set it to "https://news.google.com/news/rss/headlines/section/topic/{topic}?hl=ja&gl=JP&ceid=JP:ja"
NEWS_BASE_URL="NEWS_PROVIDER_URL"

